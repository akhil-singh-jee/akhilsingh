  <!-- JS -->
  <script type='text/javascript'><![CDATA[
  (function(){
    // Mobile menu toggle
    var mobileMenuBtn = document.getElementById('mobileMenuBtn');
    var navLinks = document.getElementById('navLinks');
    if(mobileMenuBtn){
      mobileMenuBtn.addEventListener('click', function(){ if(navLinks) navLinks.classList.toggle('active'); });
    }

    // Navbar scroll effect
    var navbar = document.getElementById('navbar');
    window.addEventListener('scroll', function(){ if(!navbar) return; if(window.scrollY > 20) navbar.classList.add('scrolled'); else navbar.classList.remove('scrolled'); });

    // Current year
    var yearEl = document.getElementById('year');
    if(yearEl) yearEl.textContent = new Date().getFullYear();

    // Smooth scroll for anchor links
    document.querySelectorAll('a[href^="#"]').forEach(function(a){
      a.addEventListener('click', function(e){
        var href = this.getAttribute('href');
        if(!href || href === '#') return;
        var target = document.querySelector(href);
        if(target){
          e.preventDefault();
          target.scrollIntoView({behavior:'smooth', block:'start'});
          if(navLinks && navLinks.classList.contains('active')) navLinks.classList.remove('active');
        }
      });
    });

    // Active nav highlight
    var sections = document.querySelectorAll('section'), navItems = document.querySelectorAll('.nav-link');
    function onScroll(){
      if(!sections.length) return;
      var idx = sections.length - 1;
      for(var i=0;i<sections.length;i++){
        var rect = sections[i].getBoundingClientRect();
        if(rect.top <= 120) idx = i;
      }
      navItems.forEach(function(it){ it.classList.remove('active'); });
      var id = (sections[idx] && sections[idx].id) ? sections[idx].id : 'home';
      var activeLink = document.querySelector('.nav-link[href="#'+id+'"]');
      if(activeLink) activeLink.classList.add('active');
    }
    onScroll();
    window.addEventListener('scroll', onScroll);

    // Close mobile menu on outside click
    document.addEventListener('click', function(e){
      if(!navLinks || !mobileMenuBtn) return;
      if(navLinks.classList.contains('active')){
        var inside = navLinks.contains(e.target) || mobileMenuBtn.contains(e.target);
        if(!inside) navLinks.classList.remove('active');
      }
    });

    // Toggle show more for sections
    function toggleSection(btnId, selector){
      var btn = document.getElementById(btnId);
      if(!btn) return;
      btn.addEventListener('click', function(){
        var parent = document.querySelector(selector);
        if(!parent) return;
        var extras = parent.querySelectorAll('[data-extra]');
        if(!extras.length) return;
        var anyHidden = Array.prototype.some.call(extras, function(el){ return el.classList.contains('hidden'); });
        extras.forEach(function(el){ if(anyHidden) el.classList.remove('hidden'); else el.classList.add('hidden'); });
        btn.textContent = anyHidden ? 'Show less' : 'Show more';
      });
    }
    toggleSection('projectsToggle', '#projects');
    toggleSection('videosToggle', '#videos');
    toggleSection('galleryToggle', '#gallery');
    toggleSection('certsToggle', '#certifications');

    // hide empty toggles
    function hideEmptyToggle(btnId, selector){
      var btn = document.getElementById(btnId); if(!btn) return;
      var parent = document.querySelector(selector); if(!parent) return;
      var extras = parent.querySelectorAll('[data-extra]'); if(!extras || extras.length === 0) btn.style.display = 'none';
    }
    hideEmptyToggle('projectsToggle','#projects');
    hideEmptyToggle('videosToggle','#videos');
    hideEmptyToggle('galleryToggle','#gallery');
    hideEmptyToggle('certsToggle','#certifications');

  })();
  ]]></script>


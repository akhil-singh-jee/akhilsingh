  <!-- Main CSS inside b:skin -->
  <b:skin><![CDATA[
    * { margin: 0; padding: 0; box-sizing: border-box; }
    body {
      font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, sans-serif;
      background-color: #111827;
      color: #f3f4f6;
      line-height: 1.6;
      transition: background-color 0.3s, color 0.3s;
    }

    a { color: inherit; text-decoration: none; }

    nav { position: fixed; top: 0; width: 100%; z-index: 1000; transition: all 0.3s; }
    nav.scrolled { background: rgba(17,24,39,0.95); backdrop-filter: blur(10px); box-shadow: 0 4px 6px rgba(0,0,0,0.1); }

    .nav-container { max-width: 1280px; margin: 0 auto; padding: 1rem 1.25rem; display:flex; justify-content:space-between; align-items:center; }
    .logo { font-size:1.5rem; font-weight:bold; background:linear-gradient(to right,#3b82f6,#9333ea); -webkit-background-clip:text; background-clip:text; -webkit-text-fill-color:transparent; }
    .nav-links { display:flex; gap:1.5rem; list-style:none; }
    .nav-links a { color:#d1d5db; text-decoration:none; font-size:0.95rem; transition:color 0.2s; }
    .nav-links a:hover, .nav-links a.active { color:#3b82f6; }
    .nav-actions { display:flex; align-items:center; gap:1rem; }
    .mobile-menu-btn { display:none; background:none; border:none; color:inherit; font-size:1.5rem; cursor:pointer; }

    .btn-cv{ background:linear-gradient(90deg,#06b6d4,#7c3aed); color:white; padding:.5rem .9rem; border-radius:.5rem; text-decoration:none; font-weight:600; font-size:0.9rem; }

    .hero { min-height:100vh; display:flex; align-items:center; justify-content:center; padding:6rem 1.25rem 2.5rem; position:relative; overflow:hidden; }
    .blob { position:absolute; width:18rem; height:18rem; border-radius:50%; filter:blur(60px); opacity:0.2; animation:blob 7s infinite; }
    .blob-1 { top:5rem; left:2.5rem; background:#3b82f6; }
    .blob-2 { top:10rem; right:2.5rem; background:#9333ea; animation-delay:2s; }
    .blob-3 { bottom:-5rem; left:50%; background:#ec4899; animation-delay:4s; }
    @keyframes blob { 0%,100%{transform:translate(0,0) scale(1);}25%{transform:translate(20px,-50px) scale(1.1);}50%{transform:translate(-20px,20px) scale(0.9);}75%{transform:translate(50px,50px) scale(1.05);} }

    .hero-content { text-align:center; position:relative; z-index:1; max-width:900px; margin:0 auto; }
    .availability-badge { display:inline-block; padding:0.4rem 1rem; background:rgba(59,130,246,0.1); border:1px solid rgba(59,130,246,0.25); border-radius:9999px; font-size:0.8rem; font-weight:600; margin-bottom:1.5rem; }
    .hero h1 { font-size:clamp(2.5rem,8vw,4.4rem); font-weight:bold; margin-bottom:1.25rem; background:linear-gradient(to right,#3b82f6,#9333ea,#ec4899); -webkit-background-clip:text; background-clip:text; -webkit-text-fill-color:transparent; line-height:1.2; }
    .hero p { font-size:clamp(1.05rem,2vw,1.35rem); color:#9ca3af; margin-bottom:2rem; max-width:48rem; margin-left:auto; margin-right:auto; }

    .cta-buttons { display:flex; flex-wrap:wrap; gap:0.75rem; justify-content:center; margin-bottom:2.5rem; }
    .btn { padding:0.85rem 1.7rem; border-radius:0.55rem; font-weight:600; text-decoration:none; transition:all 0.3s; border:none; cursor:pointer; font-size:0.95rem; display:inline-flex; align-items:center; justify-content:center; }
    .btn-primary { background:linear-gradient(to right,#3b82f6,#9333ea); color:white; }
    .btn-secondary { background:linear-gradient(to right,#ef4444,#ec4899); color:white; }
    .btn-outline { background:transparent; border:1px solid #4b5563; color:#e5e7eb; }
    .btn:hover { transform:translateY(-1px); box-shadow:0 8px 20px rgba(15,23,42,0.6); }

    .social-links { display:flex; justify-content:center; gap:1rem; }
    .social-links a { display:flex; align-items:center; justify-content:center; width:2.7rem; height:2.7rem; border-radius:999px; border:1px solid rgba(55,65,81,0.8); transition:all 0.25s; font-size:1.1rem; }
    .social-links a:hover { border-color:#3b82f6; background:rgba(37,99,235,0.15); transform:translateY(-2px); }

    .scroll-indicator { margin-top:3rem; animation:bounce 2s infinite; }
    @keyframes bounce { 0%,100%{transform:translateY(0);}50%{transform:translateY(-8px);} }

    section { padding:4.5rem 1.25rem; max-width:1280px; margin:0 auto; }
    .section-alt { background:#1f2937; max-width:none; width:100%; }
    .section-title { font-size:2.1rem; font-weight:800; text-align:center; margin-bottom:0.5rem; }
    .section-subtitle { text-align:center; color:#9ca3af; margin-bottom:2.25rem; font-size:0.95rem; }

    .about-grid { display:grid; grid-template-columns:1.2fr 1.4fr; gap:2.5rem; align-items:center; }
    .about-image { width:100%; height:21rem; background:#374151; border-radius:0.9rem; overflow:hidden; }
    .about-image img { width:100%; height:100%; object-fit:cover; }
    .stats-grid { display:grid; grid-template-columns:repeat(2,1fr); gap:1rem; margin-top:1.5rem; }
    .stat-card { padding:1.2rem; background:#111827; border-radius:0.75rem; border:1px solid rgba(55,65,81,0.9); }
    .stat-number { font-size:1.7rem; font-weight:800; }
    .stat-number.blue { color:#3b82f6; }
    .stat-number.purple { color:#9333ea; }
    .stat-number.pink { color:#ec4899; }
    .stat-number.green { color:#10b981; }
    .stat-label { font-size:0.8rem; color:#9ca3af; }

    .projects-grid { display:grid; grid-template-columns:repeat(auto-fit,minmax(260px,1fr)); gap:1.5rem; }
    .project-card { background:#111827; border-radius:0.9rem; overflow:hidden; box-shadow:0 4px 10px rgba(15,23,42,0.7); border:1px solid rgba(55,65,81,0.9); transition:transform 0.25s, box-shadow 0.25s; }
    .project-card:hover { transform:translateY(-6px); box-shadow:0 14px 35px rgba(15,23,42,0.9); }
    .project-image { height:210px; overflow:hidden; }
    .project-image img { width:100%; height:100%; object-fit:cover; display:block; transition:transform 0.4s; }
    .project-card:hover .project-image img { transform:scale(1.05); }
    .project-content { padding:1.25rem; }
    .project-title { font-weight:800; font-size:1.05rem; margin-bottom:0.4rem; background:linear-gradient(90deg,#3b82f6,#9333ea); -webkit-background-clip:text; color:transparent; }
    .project-description { color:#cbd5e1; margin-bottom:0.55rem; font-size:0.9rem; }
    .project-tags { margin-bottom:0.75rem; display:flex; gap:0.45rem; flex-wrap:wrap; }
    .tag { border:0; padding:.25rem .65rem; border-radius:999px; font-weight:600; background:rgba(31,41,55,0.9); color:#e5e7eb; cursor:pointer; font-size:0.75rem; transition:all 0.18s ease; }
    .tag:hover { transform:translateY(-2px); background:linear-gradient(90deg,#06b6d4,#7c3aed); color:#fff; }
    .tag.active { background:linear-gradient(90deg,#06b6d4,#7c3aed); color:#fff; box-shadow:0 6px 18px rgba(124,58,237,0.18); }

    .skills-grid { display:grid; grid-template-columns:repeat(auto-fit,minmax(230px,1fr)); gap:1.3rem; }
    .skill-card { padding:1.2rem; background:#111827; border-radius:0.75rem; border:1px solid rgba(55,65,81,0.9); transition:transform .18s ease, box-shadow .18s ease; }
    .skill-card:hover { transform:translateY(-4px); box-shadow:0 12px 28px rgba(15,23,42,0.9); }
    .skill-header { display:flex; align-items:center; gap:.6rem; margin-bottom:.4rem; }
    .skill-header i { font-size:1.25rem; background:linear-gradient(90deg,#06b6d4,#7c3aed); -webkit-background-clip:text; color:transparent; }
    .skill-title { font-weight:700; font-size:0.95rem; background:linear-gradient(90deg,#06b6d4,#7c3aed); -webkit-background-clip:text; color:transparent; }
    .skill-items { display:flex; gap:.45rem; flex-wrap:wrap; }
    .skill-items .tag { background:transparent; border:1px solid rgba(55,65,81,0.9); padding:.25rem .55rem; }

    .videos-grid { display:grid; grid-template-columns:repeat(auto-fit,minmax(270px,1fr)); gap:1.5rem; }
    .video-card { background:#111827; border-radius:0.9rem; overflow:hidden; border:1px solid rgba(55,65,81,0.9); cursor:pointer; transition:transform .2s ease, box-shadow .2s ease; }
    .video-card:hover { transform:translateY(-4px); box-shadow:0 14px 32px rgba(15,23,42,0.9); }
    .video-thumbnail { position:relative; height:220px; overflow:hidden; }
    .video-thumbnail img { width:100%; height:100%; object-fit:cover; display:block; }
    .play-overlay { position:absolute; inset:0; display:flex; align-items:center; justify-content:center; pointer-events:none; }
    .play-icon { background:rgba(220,38,38,0.95); color:white; font-weight:800; padding:0.55rem 0.9rem; border-radius:999px; font-size:1rem; box-shadow:0 6px 18px rgba(220,38,38,0.28); }
    .video-info { padding:0.9rem 1rem 1rem; }
    .video-title { font-size:0.98rem; font-weight:700; margin-bottom:0.2rem; }
    .video-meta { font-size:0.78rem; color:#9ca3af; }

    .gallery-grid { display:grid; grid-template-columns:repeat(auto-fit,minmax(150px,1fr)); gap:0.9rem; }
    .gallery-item { position:relative; height:150px; border-radius:0.75rem; overflow:hidden; }
    .gallery-item img { width:100%; height:100%; object-fit:cover; transition: transform 0.4s ease; display:block; }
    .gallery-item:hover img { transform: scale(1.05); }
    .gallery-short { position:absolute; top:8px; left:8px; background:rgba(0,0,0,0.6); color:#fff; padding:.2rem .45rem; border-radius:6px; font-weight:600; font-size:0.7rem; display:none; }
    .gallery-item:hover .gallery-short { display:block; }

    .certifications-grid { display:grid; grid-template-columns:repeat(auto-fit,minmax(260px,1fr)); gap:1.25rem; }
    .cert-card { display:flex; gap:1rem; align-items:center; padding:1rem; background:#111827; border-radius:0.85rem; border:1px solid rgba(55,65,81,0.9); transition:transform .18s ease, box-shadow .18s ease; }
    .cert-card:hover { transform:translateY(-4px); box-shadow:0 12px 28px rgba(15,23,42,0.9); }
    .cert-image img { width:150px; height:100px; object-fit:cover; border-radius:6px; display:block; }
    .cert-info h3 { margin:0 0 .1rem 0; font-size:0.95rem; }

    .blog-grid { display:grid; grid-template-columns:repeat(auto-fit,minmax(260px,1fr)); gap:1.3rem; }
    .blog-card { padding:1.25rem; background:#111827; border-radius:0.85rem; border:1px solid rgba(55,65,81,0.9); transition:transform .18s ease, box-shadow .18s ease; }
    .blog-card:hover { transform:translateY(-4px); box-shadow:0 12px 30px rgba(15,23,42,0.9); }
    .blog-title { font-weight:800; background: linear-gradient(90deg,#f97316,#f43f5e); -webkit-background-clip:text; color:transparent; margin-bottom:.3rem; font-size:1rem; }
    .blog-meta { font-size:0.75rem; color:#9ca3af; margin-bottom:0.15rem; }
    .blog-excerpt { font-size:0.9rem; color:#e5e7eb; }
    .read-more { display:inline-block; margin-top:.45rem; font-weight:700; font-size:0.85rem; color:#93c5fd; }

    .contact-grid { display:grid; grid-template-columns:repeat(auto-fit,minmax(230px,1fr)); gap:1.25rem; margin-bottom:2.25rem; }
    .contact-card { padding:1.25rem; background:#111827; border-radius:0.8rem; box-shadow:0 4px 16px rgba(15,23,42,0.9); text-align:center; border:1px solid rgba(55,65,81,0.9); text-decoration:none; color:inherit; display:flex; flex-direction:column; gap:0.4rem; align-items:center; justify-content:center; transition:transform .18s ease, box-shadow .18s ease; font-size:0.9rem; }
    .contact-card i { font-size:1.3rem; margin-bottom:0.25rem; }
    .contact-card h3 { font-size:0.95rem; }
    .contact-card:hover { transform:translateY(-4px); box-shadow:0 12px 28px rgba(15,23,42,0.9); }

    footer { padding:1.75rem 1.25rem 2rem; border-top:1px solid #374151; background:#020617; text-align:center; font-size:0.85rem; color:#9ca3af; margin-top:2rem; }

    .hidden { display:none !important; }

    /* Responsive */
    @media (max-width:900px){ .about-grid { grid-template-columns:1fr; } .about-image { order:-1; height:18rem; } }
    @media (max-width:768px){
      .nav-links { display:none; }
      .mobile-menu-btn { display:block; }
      .nav-links.active { display:flex; flex-direction:column; position:absolute; top:100%; left:0; right:0; background:#020617; padding:0.85rem 1.25rem 1rem; gap:0.4rem; border-bottom:1px solid #1f2937; }
      .hero { padding-top:5.5rem; }
      section { padding:3.5rem 1.1rem; }
    }
    @media (max-width:640px){
      .hero h1 { font-size:2.4rem; }
      .stats-grid { grid-template-columns:1fr 1fr; }
      .project-image { height:180px; }
      .video-thumbnail { height:200px; }
      .gallery-item { height:135px; }
      .cert-card { flex-direction:column; align-items:flex-start; }
      .cert-image img { width:100%; height:auto; }
    }
  ]]></b:skin>

</head>

<body>
  <!-- AUTO-REDIRECT MOBILE/TABLET -> DESKTOP (placed immediately after <body>) -->
  <script type='text/javascript'><![CDATA[
  (function(){
    try {
      // maximum width to consider tablet/mobile (px) - adjust if needed
      var maxTabletWidth = 1024;

      // basic UA and screen size detection
      var ua = navigator.userAgent || "";
      var isMobileUA = /Android|webOS|iPhone|iPod|BlackBerry|IEMobile|Opera Mini/i.test(ua);
      var isIpadOrTabletUA = /iPad|Tablet|PlayBook/i.test(ua);
      var isSmallScreen = Math.max(window.screen.width || 0, window.screen.height || 0) <= maxTabletWidth;
      var shouldRedirect = isMobileUA || isIpadOrTabletUA || isSmallScreen;

      if(!shouldRedirect) return;

      // Blogger's desktop link (empty if not available)
      var desktopLink = '<data:desktopLinkUrl/>' || '';

      // optional fallback to homepage if desktopLink missing:
      // if(!desktopLink) desktopLink = '<data:blog.homepageUrl/>';

      if(desktopLink && desktopLink.length) {
        // avoid redirect loop - only redirect if not already on desktop link
        if(window.location.href.indexOf(desktopLink) === -1) {
          window.location.replace(desktopLink);
        }
      }
    } catch(e) {
      // silent fail
    }
  })();
  ]]></script>

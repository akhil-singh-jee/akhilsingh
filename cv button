  <!-- JS placed here and wrapped inside CDATA (no raw & characters in the JS source) -->
  <script type='text/javascript'><![CDATA[
(function(){
  // Mobile menu toggle
  var mobileMenuBtn = document.getElementById('mobileMenuBtn');
  var navLinks = document.getElementById('navLinks');
  if(mobileMenuBtn){
    mobileMenuBtn.addEventListener('click', function(){ if(navLinks) navLinks.classList.toggle('active'); });
  }

  // Navbar scroll effect
  var navbar = document.getElementById('navbar');
  window.addEventListener('scroll', function(){
    if(!navbar) return;
    if(window.scrollY > 20) navbar.classList.add('scrolled'); else navbar.classList.remove('scrolled');
  });

  // Current year
  var yearEl = document.getElementById('year');
  if(yearEl) yearEl.textContent = new Date().getFullYear();

  // Data: URLs use \u0026 instead of & to be XML-safe
  var projects = [
    { title: 'Realtime Dashboard', desc: 'High-performance React dashboard with websockets and charting.', img: 'https://images.unsplash.com/photo-1498050108023-c5249f4df085?w=1200\u0026h=600\u0026fit=crop', tags:['React','Websockets','D3'], metrics: 'Used in production by 10k+ users', links: [{label:'Live',href:'#'},{label:'Repo',href:'#'}] },
    { title: 'Design System', desc: 'A scalable component library with theming and accessibility.', img: 'https://images.unsplash.com/photo-1547658719-da2b51169166?w=1200\u0026h=600\u0026fit=crop', tags:['TypeScript','Storybook','WCAG'], metrics: '60+ components', links: [{label:'Docs',href:'#'},{label:'Repo',href:'#'}] },
    { title: 'E-commerce PWA', desc: 'Fast, offline-capable storefront with payment integrations.', img: 'https://images.unsplash.com/photo-1593720213428-28a5b9e94613?w=1200\u0026h=600\u0026fit=crop', tags:['PWA','Next.js','Stripe'], metrics: '50% faster checkout', links: [{label:'Live',href:'#'},{label:'Case Study',href:'#'}] }
  ];

  var skills = [
    { icon: 'fab fa-react', title: 'Frontend Frameworks', items:['React','Next.js','Preact'] },
    { icon: 'fab fa-js-square', title: 'Languages \\u0026 Tooling', items:['TypeScript','JavaScript','Babel','Webpack'] },
    { icon: 'fas fa-paint-brush', title: 'Design \\u0026 UX', items:['Figma','Accessibility','Design Systems'] },
    { icon: 'fas fa-server', title: 'Infrastructure', items:['Vercel','Netlify','Docker'] }
  ];

  var videos = [
    { title: 'Building a Real-Time Chat Application', thumbnail: 'https://images.unsplash.com/photo-1611162617474-5b21e879e113?w=1200\u0026h=600\u0026fit=crop', duration: '24:15', views: '125K', url:'#' },
    { title: 'Advanced TypeScript Patterns', thumbnail: 'https://images.unsplash.com/photo-1555949963-aa79dcee981c?w=1200\u0026h=600\u0026fit=crop', duration: '18:42', views: '89K', url:'#' }
  ];

  var gallery = [
    { url: 'https://images.unsplash.com/photo-1461749280684-dccba630e2f6?w=1200\u0026h=800\u0026fit=crop', category: 'Workspace' },
    { url: 'https://images.unsplash.com/photo-1504639725590-34d0984388bd?w=1200\u0026h=800\u0026fit=crop', category: 'Design' },
    { url: 'https://images.unsplash.com/photo-1487058792275-0ad4aaf24ca7?w=1200\u0026h=800\u0026fit=crop', category: 'Conference' }
  ];

  var certs = [
    { title: 'Frontend Masters - Advanced React', issuer: 'Frontend Masters', year: '2023' },
    { title: 'Google - Web Performance', issuer: 'Google', year: '2022' }
  ];

  var posts = [
    { title: 'Optimizing React Rendering', date: '2024-11-10', excerpt: 'Techniques to reduce re-renders and improve FPS.' },
    { title: 'Design Systems at Scale', date: '2024-08-22', excerpt: 'How to maintain consistency across large apps.' }
  ];

  // Helper
  function el(tag, cls, html){
    var d = document.createElement(tag);
    if(cls) d.className = cls;
    if(html) d.innerHTML = html;
    return d;
  }

  function escapeHtml(str){
    if(typeof str !== 'string') return str;
    return str.replace(/\u0026/g,'&amp;').replace(/</g,'&lt;').replace(/>/g,'&gt;').replace(/"/g,'&quot;').replace(/'/g,'&#39;');
  }

  function renderProjects(){
    var container = document.getElementById('projectsGrid');
    if(!container) return;
    projects.forEach(function(p){
      var card = el('div','project-card',
        '<div class="project-image"><img src="'+p.img+'" alt="'+escapeHtml(p.title)+'"></div>' +
        '<div class="project-content">' +
          '<div class="project-title">'+escapeHtml(p.title)+'</div>' +
          '<div class="project-description">'+escapeHtml(p.desc)+'</div>' +
          '<div class="project-tags">'+p.tags.map(function(t){ return '<span class="tag">'+escapeHtml(t)+'</span>'; }).join('')+'</div>' +
          '<div class="project-metrics"><p>'+escapeHtml(p.metrics)+'</p></div>' +
          '<div class="project-links">'+p.links.map(function(l){ return '<a href="'+l.href+'">'+escapeHtml(l.label)+'</a>'; }).join('')+'</div>' +
        '</div>'
      );
      container.appendChild(card);
    });
  }

  function renderSkills(){
    var container = document.getElementById('skillsGrid');
    if(!container) return;
    skills.forEach(function(s){
      var card = el('div','skill-card',
        '<div class="skill-header"><i class="'+s.icon+'"></i><div class="skill-title">'+escapeHtml(s.title)+'</div></div>' +
        '<div class="skill-items">'+s.items.map(function(i){ return '<span class="tag">'+escapeHtml(i)+'</span>'; }).join('')+'</div>'
      );
      container.appendChild(card);
    });
  }

  function renderVideos(){
    var container = document.getElementById('videosGrid');
    if(!container) return;
    videos.forEach(function(v){
      var card = el('div','video-card',
        '<div class="video-thumbnail"><img src="'+v.thumbnail+'" alt="'+escapeHtml(v.title)+'">' +
        '<div class="video-overlay"><div class="play-button"><div class="play-icon"></div></div></div>' +
        '<div class="video-duration">'+escapeHtml(v.duration)+'</div></div>' +
        '<div class="video-info"><div class="video-title">'+escapeHtml(v.title)+'</div><div class="video-meta">'+escapeHtml(v.views)+' views</div></div>'
      );
      container.appendChild(card);
    });
  }

  function renderGallery(){
    var container = document.getElementById('galleryGrid');
    if(!container) return;
    gallery.forEach(function(g){
      var item = el('div','gallery-item',
        '<img src="'+g.url+'" alt="'+escapeHtml(g.category)+'">' +
        '<div class="gallery-overlay"><div class="gallery-category">'+escapeHtml(g.category)+'</div></div>'
      );
      container.appendChild(item);
    });
  }

  function renderCerts(){
    var container = document.getElementById('certGrid');
    if(!container) return;
    certs.forEach(function(c){
      var card = el('div','cert-card',
        '<div class="cert-icon"><i class="fas fa-certificate"></i></div>' +
        '<div class="cert-info"><h3>'+escapeHtml(c.title)+'</h3><div class="cert-issuer">'+escapeHtml(c.issuer)+'</div><div class="cert-year">'+escapeHtml(c.year)+'</div></div>'
      );
      container.appendChild(card);
    });
  }

  function renderBlogs(){
    var container = document.getElementById('blogGrid');
    if(!container) return;
    posts.forEach(function(p){
      var card = el('article','blog-card',
        '<div class="blog-meta"><i class="fas fa-calendar-alt"></i> '+escapeHtml(p.date)+'</div>' +
        '<div class="blog-title">'+escapeHtml(p.title)+'</div>' +
        '<div class="blog-excerpt">'+escapeHtml(p.excerpt)+'</div>' +
        '<a class="blog-link" href="#">Read more <i class="fas fa-arrow-right"></i></a>'
      );
      container.appendChild(card);
    });
  }

  // Init renders
  renderProjects(); renderSkills(); renderVideos(); renderGallery(); renderCerts(); renderBlogs();

  // Smooth scroll for anchor links
  document.querySelectorAll('a[href^="#"]').forEach(function(a){
    a.addEventListener('click', function(e){
      var href = this.getAttribute('href');
      if(!href || href === '#') return;
      var target = document.querySelector(href);
      if(target){
        e.preventDefault();
        target.scrollIntoView({behavior:'smooth', block:'start'});
        if(navLinks && navLinks.classList.contains('active')) navLinks.classList.remove('active');
      }
    });
  });

  // Active nav highlight
  var sections = document.querySelectorAll('section'), navItems = document.querySelectorAll('.nav-link');
  function onScroll(){
    if(!sections.length) return;
    var idx = sections.length - 1;
    for(var i=0;i<sections.length;i++){
      var rect = sections[i].getBoundingClientRect();
      if(rect.top <= 120) idx = i;
    }
    navItems.forEach(function(it){ it.classList.remove('active'); });
    var id = (sections[idx] && sections[idx].id) ? sections[idx].id : 'home';
    var activeLink = document.querySelector('.nav-link[href="#'+id+'"]');
    if(activeLink) activeLink.classList.add('active');
  }
  onScroll();
  window.addEventListener('scroll', onScroll);


  // Accessibility: keyboard CV button opens in new tab; ensure download attribute used by browsers
  var cvBtn = qs('#cvBtn');
  if(cvBtn){
    cvBtn.addEventListener('keydown', function(e){
      if(e.key === 'Enter' || e.key === ' '){ /* let default anchor behavior handle */ }
    });
  }

  // Close mobile menu on outside click
  document.addEventListener('click', function(e){
    if(!navLinksEl || !mobileBtn) return;
    if(navLinksEl.classList.contains('show')){
      var inside = navLinksEl.contains(e.target) || mobileBtn.contains(e.target);
      if(!inside) navLinksEl.classList.remove('show');
    }
  });

})();
]]></script>

